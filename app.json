{
  "expo": {
    "name": "Next Students",
    "slug": "bismillah",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logofull.png",
    "scheme": "bismillah",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logofull.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.twizz.bismillah",
      "windowSoftInputMode": "adjustPan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logofull.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logofull.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f9f704d7-5680-4009-bb79-34a8da551690"
      }
    }
  }
}
